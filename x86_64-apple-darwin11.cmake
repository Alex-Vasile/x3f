SET(CMAKE_SYSTEM_NAME Darwin-GNU)
SET(CMAKE_C_COMPILER x86_64-apple-darwin11-gcc)
SET(CMAKE_CXX_COMPILER x86_64-apple-darwin11-g++)
SET(_CMAKE_TOOLCHAIN_PREFIX x86_64-apple-darwin11-)
EXECUTE_PROCESS(COMMAND x86_64-apple-darwin11-gcc -print-sysroot OUTPUT_VARIABLE CMAKE_FIND_ROOT_PATH OUTPUT_STRIP_TRAILING_WHITESPACE)
SET(CMAKE_FIND_ROOT_PATH_MODE_PROGRAM NEVER)
SET(CMAKE_FIND_ROOT_PATH_MODE_LIBRARY ONLY)
SET(CMAKE_FIND_ROOT_PATH_MODE_INCLUDE ONLY)
